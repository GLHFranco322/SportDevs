{
  "name": "eventos",
  "version": "0.0.0",
  "private": true,
  "main": "src/bin/www",
  "scripts": {
    "start": "node ./src/bin/www",
    "dev": "npm-run-all --parallel dev:server dev:browser-sync",
    "dev:server": "nodemon",
    "dev:browser-sync": "browser-sync start --proxy 'localhost:3000' --files 'public/**/*' 'views/**/*' --no-notify --no-ui",
    "db-init": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "db-reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.18.1",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.0",
    "sequelize": "^6.37.6",
    "sequelize-cli": "^6.6.2",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5"
  }
}
