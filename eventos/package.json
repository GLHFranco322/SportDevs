{
  "name": "eventos",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "npm-run-all --parallel dev:server dev:browser-sync",
    "dev:server": "nodemon",
    "dev:browser-sync": "browser-sync start --proxy 'localhost:3000' --files 'public/**/*' 'views/**/*' --no-notify --no-ui"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "browser-sync": "^2.27.10",
    "npm-run-all": "^4.1.5"
  }
}